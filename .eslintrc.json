{
    "env": {
    "node": true,
      "es6": true
  },
    "parserOptions": {
    "ecmaVersion": 2017
  },
    "rules": {
    "no-console": 1,
      "no-unused-vars": 0,
      "no-var": 0,
      "linebreak-style": 0,
      "spaced-comment": 0,
      "strict": 0,
      "quotes": 0,
      "max-len": 0,
      "object-curly-spacing": 0,
      "no-multi-spaces": 0,
      "prefer-template": 0,
      "no-undef": 0,
      "no-underscore-dangle": 0,
      "no-return-await": 0,
      "consistent-return": 0,
      "prefer-destructuring": 0,
      "brace-style": 0,
      "no-lonely-if": 0,
      "camelcase": 0,
      "no-param-reassign": 0,
      "class-methods-use-this": 0,
      "no-plusplus": 0,
      "no-await-in-loop": 0,
      "global-require": 0
  }
}